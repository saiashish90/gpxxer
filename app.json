{
  "expo": {
    "name": "gpxxer",
    "slug": "gpxxer",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "gpxxer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen"
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for bike tracking."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
